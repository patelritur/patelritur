<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="bookingDetail"
            type="com.demo.home.booking.model.BookingAcceptModel" />
        <import type="android.view.View"/>
    </data>
    <androidx.core.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:fillViewport="true"
        android:layout_height="match_parent">


        <androidx.appcompat.widget.LinearLayoutCompat
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:id="@+id/parent_ll"
            android:layout_height="match_parent"
            xmlns:tools="http://schemas.android.com/tools"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:orientation="vertical">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/ic_mask"
                >



                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">


                <androidx.appcompat.widget.AppCompatTextView
                    android:text="@{bookingDetail.bookingMessage}"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/montserrat_bold"
                    android:textStyle="bold"
                    android:gravity="center_horizontal"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="@color/white"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="3.5sp"
                    android:layout_marginStart="@dimen/margin_20"
                    android:layout_marginEnd="@dimen/margin_20"
                    android:layout_marginBottom="@dimen/margin_10"
                    android:layout_marginTop="@dimen/margin_10"
                    tools:text="Your Demo is booked!"/>

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/call"
                        android:layout_marginEnd="@dimen/margin_10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_call"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/submessage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:visibility="gone"
                    android:layout_marginBottom="@dimen/margin_5"
                    android:fontFamily="@font/montserrat_regular"/>

                <View
                    android:layout_marginTop="@dimen/margin_10"
                    android:layout_height="3dp"
                    android:layout_marginBottom="@dimen/margin_10"

                    style="@style/drawer"/>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/margin_10"
                    android:paddingBottom="@dimen/margin_10"
                    android:orientation="horizontal">
                    <FrameLayout
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:background="@drawable/white_circle">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="@dimen/margin_30"
                            android:src="@drawable/red_circle"
                            android:layout_gravity="center"
                            app:roundHomeOptionsImage="@{bookingDetail.bookingdetails.specialistImage}"
                            android:layout_height="@dimen/margin_30"
                            />
                    </FrameLayout>

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/margin_10"
                        android:layout_height="@dimen/margin_10"
                        android:tint="@color/white"
                        android:layout_marginStart="@dimen/margin_5"
                        android:layout_marginEnd="5dp"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/ic_baseline_keyboard_arrow_left_24"/>

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_weight="0.5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                    <androidx.appcompat.widget.AppCompatTextView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:layout_gravity="center_vertical"
                        android:text='@{bookingDetail.bookingSubMessage.trim().length()>0 ? bookingDetail.bookingSubMessage : bookingDetail.bookingdetails.specialistName}'
                        android:fontFamily="@font/montserrat_regular"
                        />
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat_regular"
                        android:textColor="@color/white"
                        android:textSize="12sp"
                        android:id="@+id/distance_text"/>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/status"
                        android:layout_marginStart="@dimen/margin_20"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat_regular"
                        android:padding="@dimen/margin_5"
                        android:layout_gravity="end"
                        android:gravity="end"
                        android:textColor="@color/white"
                        android:text="Booked"
                        android:background="@drawable/background_status_booking"
                        />
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/arriving_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:visibility="gone"
                    android:layout_marginBottom="@dimen/margin_10"
                    android:fontFamily="@font/montserrat_regular"
                    />
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:visibility="gone"
                    android:id="@+id/ll_timeSlot"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginBottom="@dimen/margin_10"
                    android:orientation="horizontal">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textSize="12sp"
                        tools:text="erere"
                        android:fontFamily="@font/montserrat_regular"
                        android:text='@{"Booking Time: " +bookingDetail.bookingTime}'
                        />
                    <androidx.appcompat.widget.AppCompatTextView
                        android:text="edee"
                        android:id="@+id/arriving_timestamp"
                        android:layout_width="0dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textColor="@color/white"
                        android:textSize="12sp"
                        android:fontFamily="@font/montserrat_regular"
                        />
                </androidx.appcompat.widget.LinearLayoutCompat>

            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:visibility='@{bookingDetail.bookingdetails.isShowTemp ? View.VISIBLE :View.GONE}'
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <View style="@style/divider"/>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1"
                    android:layout_marginBottom="@dimen/margin_5"
                    android:orientation="horizontal">

                    <HorizontalScrollView
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:scrollbars="horizontal"
                        android:scrollbarSize="1dp"

                        >
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/flow_layout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:orientation="horizontal"/>
                    </HorizontalScrollView>
                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/next_arrow"
                        android:visibility="gone"
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

                    </androidx.appcompat.widget.AppCompatImageView>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <View style="@style/divider"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="@dimen/margin_5"
                android:layout_margin="@dimen/margin_20"
                android:padding="@dimen/margin_20"
                >
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:id="@+id/cardetail_ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_weight="0.9"
                        android:layout_width="wrap_content"
                        android:layout_margin="@dimen/margin_20"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:text="@{bookingDetail.bookingdetails.carName}"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/montserrat_bold"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:textColor="#222831"
                            tools:text="Maruti Brezza"/>
                        <TextView
                            android:text="@{bookingDetail.bookingdetails.fuleType}"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/montserrat_regular"
                            android:textStyle="normal"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:textColor="#4c4c4c"
                            tools:text="Petrol"/>
                        <TextView
                            android:text="@{bookingDetail.bookingdetails.faceliftDesc}"
                            android:background="#689e5a"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/montserrat_semibold"
                            android:textStyle="normal"
                            android:paddingStart="@dimen/margin_10"
                            android:paddingEnd="@dimen/margin_10"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:textColor="#ffffff"
                            tools:text="FACELIFT"/>
                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/performa_invoice"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/performa_invoice"
                            android:layout_marginTop="@dimen/margin_5"
                            android:textColor="@color/color_241e61"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            android:fontFamily="@font/montserrat_regular"
                            />
                    </androidx.appcompat.widget.LinearLayoutCompat>
                    <androidx.appcompat.widget.AppCompatImageView
                        app:homeOptionsImage="@{bookingDetail.bookingdetails.carImage}"
                        android:layout_gravity="center_vertical"
                        android:layout_width="110dp"
                        android:layout_height="80dp"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="@dimen/margin_5"
                android:layout_marginStart="@dimen/margin_20"
                android:layout_marginEnd="@dimen/margin_20"
                android:layout_marginTop="@dimen/margin_20"
                android:padding="@dimen/margin_20"
                android:layout_marginBottom="@dimen/margin_10"
                >

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"

                    >

                    <TextView
                        android:padding="@dimen/margin_20"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat_semibold"
                        android:textStyle="normal"
                        android:textAppearance="@style/TextAppearance.AppCompat.Small"
                        android:textColor="#474747"
                        android:text="@string/play_Recording"/>


                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/play_voice_message"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginEnd="@dimen/margin_10"
                        android:src="@drawable/ic_play"/>

                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/new_voice_message"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_20"
                    android:text="New Voice message"/>
            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:id="@+id/recording_cardview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="@dimen/margin_5"
                android:layout_marginEnd="@dimen/margin_20"
                android:layout_marginBottom="@dimen/margin_20"
                android:layout_marginStart="@dimen/margin_20"
                android:layout_marginTop="@dimen/margin_10"
                android:padding="@dimen/margin_20"
                android:visibility='@{bookingDetail.bookingdetails.voiceMessageStatus.equalsIgnoreCase("Y") ? View.VISIBLE : View.GONE }'

                >
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/margin_20"
                        android:textColor="#393939"
                        android:fontFamily="@font/montserrat_semibold"
                        android:textAppearance="@style/TextAppearance.AppCompat.Small"
                        android:text="@string/leave_a_voice_message"
                        />


                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/record_audio"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginEnd="@dimen/margin_10"
                            android:src="@drawable/ic_record" />


                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:visibility='@{bookingDetail.bookingdetails.specialistID.equalsIgnoreCase("0") ? View.GONE : View.VISIBLE}'
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_20"
                android:layout_marginEnd="@dimen/margin_20"

                app:cardElevation="@dimen/margin_5">
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_10"
                    android:orientation="vertical">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <androidx.appcompat.widget.AppCompatImageView
                            app:roundHomeOptionsImage="@{bookingDetail.bookingdetails.specialistImage}"
                            android:layout_marginStart="@dimen/margin_20"
                            android:layout_width="@dimen/margin_40"
                            android:layout_marginEnd="@dimen/margin_15"
                            android:layout_height="@dimen/margin_40"/>
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/margin_10"
                            android:orientation="vertical">
                            <TextView
                                android:text="@{bookingDetail.bookingdetails.specialistName}"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/montserrat_semibold"
                                android:textStyle="normal"
                                android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                android:textColor="#474747"
                                tools:text="Ajay Singh"/>
                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/margin_5">

                                <RatingBar
                                    android:id="@+id/rating"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:stepSize="1"
                                    android:numStars="5"
                                    android:layout_marginEnd="@dimen/margin_30"
                                    android:theme="@style/RatingBar"
                                    android:layout_gravity="center_vertical"
                                    style="@android:style/Widget.Material.RatingBar.Small"
                                    app:carRating="@{bookingDetail.bookingdetails.specialistRating}"
                                    />
                                <TextView
                                    android:text="@{bookingDetail.bookingdetails.specialistTotalBooking}"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/montserrat_bold"
                                    android:layout_gravity="center_vertical"
                                    android:textStyle="normal"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                    android:textColor="#474747"
                                    tools:text="30"/>
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/montserrat_semibold"
                                    android:textStyle="normal"
                                    android:layout_gravity="center_vertical|end"
                                    android:singleLine="true"
                                    android:autoSizeTextType="uniform"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                    android:textColor="#474747"
                                    android:text=" Demos completed"/>
                            </androidx.appcompat.widget.LinearLayoutCompat>


                        </androidx.appcompat.widget.LinearLayoutCompat>


                    </androidx.appcompat.widget.LinearLayoutCompat>

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1"
                        android:layout_marginTop="@dimen/margin_10"
                        android:layout_marginBottom="@dimen/margin_15"
                        android:orientation="horizontal">
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_weight="0.9"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:text="@{bookingDetail.bookingdetails.companyName}"
                                android:layout_marginStart="@dimen/margin_20"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                                android:fontFamily="@font/montserrat_semibold"
                                android:textStyle="normal"
                                android:textColor="#474747"
                                tools:text="DD MOTORS"/>
                            <TextView
                                android:layout_marginStart="@dimen/margin_20"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/montserrat_regular"
                                android:textStyle="normal"
                                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                                android:textColor="#474747"
                                android:text="@{bookingDetail.bookingdetails.companyAddress}"/>
                        </androidx.appcompat.widget.LinearLayoutCompat>
                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_marginStart="@dimen/margin_10"
                            android:layout_marginEnd="@dimen/margin_10"
                            app:homeOptionsImage="@{bookingDetail.bookingdetails.companyLogo}"
                            android:layout_width="@dimen/margin_60"
                            android:scaleType="centerCrop"
                            android:layout_gravity="center_horizontal|end"
                            android:layout_height="@dimen/margin_60"/>
                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardElevation="@dimen/margin_5"
                android:layout_margin="@dimen/margin_20"
                android:padding="@dimen/margin_20"
                android:id="@+id/caradress_ll"
                android:visibility='@{bookingDetail.bookingdetails.demoType.equalsIgnoreCase("At Home")? View.VISIBLE : View.GONE}'

                >
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/margin_20"
                        >

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/destination"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/montserrat_bold"
                            android:text="Destination:"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:textColor="#474747" />
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_toEndOf="@+id/destination"
                            android:id="@+id/house"
                            android:layout_width="wrap_content"
                            android:gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/montserrat_semibold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                            android:textColor="#474747"
                            android:text=" Your House"/>

                        <androidx.appcompat.widget.AppCompatEditText
                            android:id="@+id/address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/destination"
                            android:layout_alignEnd="@+id/house"
                            android:background="@null"
                            android:enabled="false"
                            android:imeOptions="actionDone"
                            android:fontFamily="@font/montserrat_regular"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            android:textColor="#474747" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/change_address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/change_address"
                            android:textSize="12sp"
                            android:fontFamily="@font/montserrat_regular"
                            android:textColor="#474747"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentBottom="true"

                            />
                    </RelativeLayout>

                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.cardview.widget.CardView>

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_marginTop="@dimen/margin_20"
                android:visibility="gone"
                android:elevation="@dimen/margin_10"
                android:id="@+id/join_call"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/join_call"
                android:layout_gravity="center_horizontal"
                style="@style/redButton"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_marginTop="@dimen/margin_20"
                android:visibility="gone"
                android:elevation="@dimen/margin_10"
                android:id="@+id/book_demo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/takedemos"
                android:layout_gravity="center_horizontal"
                style="@style/redButton"/>
            <View
                style="@style/divider"/>
            <TextView
                android:layout_marginStart="@dimen/margin_20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_regular"
                android:textStyle="bold"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                android:textColor="#393939"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="3sp"
                android:text="@string/tip_click_here_to_see_how_to_get_the_most_out_of_your_demo_experience"/>
            <View style="@style/divider"/>

            <TextView
                style="@style/redButton"
                android:id="@+id/tv_left_home"
                android:visibility='@{bookingDetail.bookingdetails.demoType.equalsIgnoreCase("At Dealership")? View.VISIBLE : View.GONE}'
                android:layout_width="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:textStyle="normal"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="5sp"
                android:layout_marginBottom="@dimen/margin_20"
                android:gravity="center_horizontal"
                android:text="@{bookingDetail.bookingdetails.meetingType!=null ? @string/left_home_meeting :  @string/left_home_demo }"

                />

            <TextView
                android:id="@+id/tv_cancel"
                android:visibility='@{bookingDetail.bookingdetails.cancelBtnStatus.equalsIgnoreCase("Y")? View.VISIBLE : View.GONE}'
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_regular"
                android:textStyle="normal"
