<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="carCheckList"
            type="com.demo.home.booking.model.BookingAcceptModel.Bookingdetails.CarCheckList" />
    </data>

    <androidx.appcompat.widget.LinearLayoutCompat
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_marginTop="@dimen/margin_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatImageView
                app:homeOptionsImage="@{carCheckList.CheckListIcon}"
                android:layout_marginStart="@dimen/margin_20"
                android:layout_marginEnd="@dimen/margin_10"
                android:layout_width="20dp"
                android:layout_height="40dp"
                android:tint="@color/color_241e61"
                android:src="@mipmap/image_sanitize"/>

            <TextView
                android:layout_gravity="center_vertical"
                android:text="@{carCheckList.CheckListValue}"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/montserrat_regular"
                android:textSize="12sp"
                android:textColor="#393939"
                android:lineSpacingExtra="1sp"
                tools:text="Temperature 98 fgfddffhgfjhasdsa "/>
        </androidx.appcompat.widget.LinearLayoutCompat>

    </androidx.appcompat.widget.LinearLayoutCompat>
</layout>
